{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch:types": "openapi-zod-client http://localhost:3000/swagger/json --group-strategy=tag-file -o ./src/interfaces/openapi-zod-client.ts  --export-schemas=true export-types=true --base-url=http://localhost:3000 --strict-objects",
