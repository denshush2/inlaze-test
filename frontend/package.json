    "@mui/icons-material": "^6.3.0",
    "@mui/lab": "6.0.0-beta.21",
    "@mui/material": "^6.3.0",
    "@mui/x-date-pickers": "^7.23.3",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@zodios/core": "^10.9.6",
    "axios": "^1.7.9",
    "date-fns": "2.30.0",
    "openapi-zod-client": "^1.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
